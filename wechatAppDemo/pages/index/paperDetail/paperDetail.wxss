.container {
  padding: 20rpx;
  background-color: #f5f5f5;
  min-height: 100vh;
}

.paper-title {
  font-size: 34rpx;
  font-weight: bold;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #eee;
  margin-bottom: 30rpx;
}

.year-list {
  display: flex;
  flex-direction: column;
  gap: 16rpx;
}

.year-item {
  background-color: white;
  padding: 24rpx;
  border-radius: 12rpx;
  font-size: 28rpx;
  color: #333;
  box-shadow: 0 2rpx 8rpx rgba(0,0,0,0.05);
}